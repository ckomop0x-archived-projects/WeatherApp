{
  "name": "weather-app",
  "author": "Pavel \"Ckomop0x\" Klochkov <paul.klochkov@gmail.com> (https://ckomop0x.github.io/)",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/ckomop0x/WeatherApp#readme",
  "license": "MIT",
  "devDependencies": {
    "commitizen": "3.1.1",
    "cypress": "3.2.0",
    "cypress-react-unit-test": "2.4.1",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-to-json": "^3.3.5",
    "gh-pages": "^2.0.1",
    "ghooks": "2.0.4",
    "lint-staged": "8.1.6",
    "puppeteer": "1.14.0",
    "react-addons-test-utils": "^15.6.2",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "semantic-release": "15.13.12"
  },
  "dependencies": {
    "@material-ui/core": "3.9.3",
    "@material-ui/styles": "3.0.0-alpha.10",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.6",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.2",
    "husky": "2.2.0",
    "i18next": "15.1.1",
    "i18next-browser-languagedetector": "3.0.1",
    "i18next-xhr-backend": "2.0.1",
    "nprogress": "0.2.0",
    "prettier": "1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "10.9.0",
    "react-scripts": "2.1.8",
    "react-test-renderer": "^16.8.6",
    "serve": "11.0.0",
    "styled-components": "4.2.0",
    "typescript": "3.4.5"
  },
  "scripts": {
    "start": "PORT=9101 react-scripts start",
    "commit": "git-cz",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage --watchAll",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:write": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "eject": "react-scripts eject",
    "test:coverage": "react-scripts test --coverage",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test:coverage"
    }
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
