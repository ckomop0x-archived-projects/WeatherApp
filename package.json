{
  "name": "weather-app",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://ckomop0x.github.io/WeatherApp/",
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-to-json": "^3.3.5",
    "gh-pages": "^2.0.1",
    "ghooks": "2.0.4",
    "puppeteer": "1.14.0",
    "react-addons-test-utils": "^15.6.2",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8"
  },
  "dependencies": {
    "@material-ui/core": "3.9.3",
    "@material-ui/styles": "3.0.0-alpha.10",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.6",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "react-test-renderer": "^16.8.6",
    "serve": "11.0.0",
    "typescript": "3.4.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage --watchAll",
    "eject": "react-scripts eject",
    "test:coverage": "react-scripts test --coverage",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "config": {
    "ghooks": {
      "pre-commit": "yarn test:coverage"
    }
  }
}
